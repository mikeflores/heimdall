
<h2>Projects</h2>
<ul>
  <% @projects.each do |project|%>
    <li>
      <ul>
        <li><%= link_to project.name, project_path(project) %></li>
        <li><%= link_to project.total_hours %></li>
        <li><%= link_to "edit", edit_project_path(project) %></li>
        <li><%= link_to "delete", project_path(project), method: :delete %></li>
      </ul>
    </li>
  <% end %>
</ul>